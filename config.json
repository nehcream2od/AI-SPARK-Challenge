{
    "name": "AI-SPARK-Challenge GANModel",
    "arch": {
        "type": "GANModel",
        "args": {
            "input_size": null,
            "output_size": null,
            "gen_hidden_size": [
                16,
                8
            ],
            "dsc_hidden_size": [
                8
            ]
        }
    },
    "seed": 1215,
    "data_module": {
        "type": "CustomDataModule",
        "args": {
            "data_dir": "./data",
            "batch_size": 32,
            "shuffle": true,
            "num_workers": 0,
            "prepare_data_per_node": true,
            "preprocess_fn": {
                "scaler": "robust",
                "scaler_comment": [
                    "False",
                    "minmax",
                    "standard",
                    "robust",
                    "gaussian",
                    "log"
                ],
                "fourier_transform": {
                    "apply": false,
                    "features": [
                        "motor_current",
                        "motor_rpm",
                        "motor_vibe"
                    ]
                }
            },
            "kfold": {
                "n_splits": 5,
                "shuffle": true,
                "random_state": 42
            }
        }
    },
    "gen_optimizer": {
        "type": "Lion",
        "args": {
            "lr": 0.000001
        }
    },
    "disc_optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.000001
        }
    },
    "gen_scheduler": {
        "type": "CosineAnnealingWarmUpRestarts",
        "args": {
            "T_0": 100,
            "T_mult": 2,
            "eta_max": 0.001,
            "T_up": 10,
            "gamma": 0.5
        }
    },
    "disc_scheduler": {
        "type": "CosineAnnealingWarmUpRestarts",
        "args": {
            "T_0": 100,
            "T_mult": 2,
            "eta_max": 0.003,
            "T_up": 10,
            "gamma": 0.5
        }
    },
    "criterion_gen": "mean_squared_error",
    "criterion_disc": "binary_cross_entropy_with_logits",
    "alpha": 0.7,
    "threshold": 0.99,
    "trainer": {
        "accelerator": "cpu",
        "devices": 1,
        "max_epochs": 100,
        "save_dir": "saved/"
    }
}
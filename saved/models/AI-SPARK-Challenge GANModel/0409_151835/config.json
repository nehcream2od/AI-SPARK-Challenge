{
    "name": "AI-SPARK-Challenge GANModel",
    "arch": {
        "type": "GANModel",
        "args": {
            "input_size": null,
            "output_size": null,
            "gen_hidden_size": [
                16,
                8
            ],
            "dsc_hidden_size": [
                8
            ]
        }
    },
    "data_module": {
        "type": "CustomDataModule",
        "args": {
            "data_dir": "/Users/nehcream/Documents/Workspace/Active/AI-SPARK-Challenge/data",
            "batch_size": 32,
            "shuffle": true,
            "num_workers": 8,
            "prepare_data_per_node": false,
            "preprocess_fn": {
                "scaler": "standard",
                "scaler_comment": [
                    "False",
                    "minmax",
                    "standard",
                    "robust",
                    "gaussian",
                    "log"
                ],
                "fourier_transform": {
                    "apply": false,
                    "features": [
                        "motor_current",
                        "motor_rpm",
                        "motor_vibe"
                    ]
                }
            },
            "kfold": {
                "n_splits": 5,
                "shuffle": true,
                "random_state": 42
            }
        }
    },
    "gen_optimizer": {
        "type": "Lion",
        "args": {
            "lr": 0.001
        }
    },
    "disc_optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "criterion_gen": "mean_squared_error",
    "criterion_dsc": "binary_cross_entropy_with_logits",
    "alpha": 0.5,
    "metrics": [
        "accuracy",
        "top_k_acc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "accelerator": "cpu",
        "devices": 1,
        "max_epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}